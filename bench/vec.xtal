Vec2D: class{
  + _x;
  + _y;

  initialize: method(_x: 0, _y: 0){}

  op_add#this: method(v){
    return Vec2D(_x+v.x, _y+v.y);
  }
  
  op_add_assign#this: method(v){
    _x += v.x;
    _y += v.y;
    return this;
  }
  
  to_s: method(){
    return %f(Vec2D(x: %g, y: %g))(_x, _y);
  }
}

foo: fun(){
	v: Vec2D(0, 0);
	u: Vec2D(1, 2);
	for(i: 0; i<5000000; ++i){
		v += u;
		//v = v + u;
	}
	
	v.p;
}


foo();

